request:
curl -X "POST" -i -H "Content-Type: application/json" -H "X-Auth-Token: $OS_TOKEN" -d '{"service":{"type":"demo","name":"demo","description":"demo"}}' http://localhost:5000/v3/services; echo

response:
{
   "service":{
      "name":"demo",
      "links":{
         "self":"http://192.168.126.2:5000/v3/services/7bcc585420264cf499ab5e56a1632471"
      },
      "enabled":true,
      "type":"demo",
      "id":"7bcc585420264cf499ab5e56a1632471",
      "description":"demo"
   }
}
