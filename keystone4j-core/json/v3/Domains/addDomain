request:
curl -X "POST" -i -H "Content-Type: application/json" -H "X-Auth-Token: $OS_TOKEN" -d '{"domain":{"name":"demo","description":"demo"}}' http://localhost:5000/v3/domains;

response:
{
   "domain":{
      "links":{
         "self":"http://192.168.126.2:5000/v3/domains/5b473a7c5ff0435b8f1e216e62b8e4e0"
      },
      "enabled":true,
      "description":"demo",
      "name":"demo",
      "id":"5b473a7c5ff0435b8f1e216e62b8e4e0"
   }
}