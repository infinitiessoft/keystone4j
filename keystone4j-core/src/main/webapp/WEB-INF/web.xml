<?xml version="1.0" encoding="UTF-8"?>
<web-app id="keystone4j" version="3.0"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<listener>
		<listener-class>com.infinities.keystone4j.jpa.EntityManagerListener</listener-class>
	</listener>
	<servlet>
		<servlet-name>keystone4j</servlet-name>
		<servlet-class>org.glassfish.jersey.servlet.ServletContainer</servlet-class>
		<init-param>
			<param-name>javax.ws.rs.Application</param-name>
			<param-value>com.infinities.keystone4j.KeystoneApplication</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>keystone4j</servlet-name>
		<url-pattern>/*</url-pattern>
	</servlet-mapping>
</web-app>